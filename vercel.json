{
  "functions": {
    "dist/server.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/dist/server.js"
    },
    {
      "src": "/tools",
      "dest": "/dist/server.js"
    },
    {
      "src": "/message",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "buildCommand": "npm run build"
}