.aura-logo[data-animate="true"] .aura-logo__flower,
.aura-logo[data-interactive="true"]:hover .aura-logo__star,
.aura-logo[data-interactive="true"]:hover .aura-logo__flower{
  animation:logo-animation--combined 3s cubic-bezier(.4, 0, .2, 1) infinite;
}

